[alias]
    s = status
    d = diff
    dc = diff --cached
    dw = diff --word-diff
    dcw = diff --cached --word-diff
    ci = commit
    hist = log --graph --pretty=format:'%C(yellow)%h%C(cyan)%d%Creset %s %C(white)- %an, %ar%Creset'
    hh = log --graph --pretty=format:'%C(yellow)%h: %Cgreen%ad%Cred%d %Creset%n%s%Cblue //%cn%n' --decorate --date=short
    b = branch
    ch = checkout
    fetchfix = config remote.origin.fetch "+refs/heads/*:refs/remotes/origin/*"
